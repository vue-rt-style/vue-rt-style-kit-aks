        max-height 100vh
      @media(max-width tablet-upper-limit)
        width 100%
        z-index 0
        overflow hidden
        img
          object-fit cover
          min-width 100%
          min-height 100%
      @media(max-width mobile-upper-limit)
        position relative
        width 100%
        z-index 0
        height auto
        img
          max-width 100%
        .rt-logo
